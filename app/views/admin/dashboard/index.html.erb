<h2>Admin Dashboard</h2>

<p><%= link_to "Merchants", "/admin/merchants" %></p>
<p><%= link_to "Invoices", "/admin/invoices" %></p>
<p>Incomplete Invoices:
  <% @invoices.each do |invoice| %>
    <% invoice.incomplete_invoices.each do |invoice_item| %>
      <%= link_to "#{invoice_item.invoice_id}", "/admin/invoices/#{invoice_item.invoice_id}" %>
    <% end %>
  <% end %>
</p>
